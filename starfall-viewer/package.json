    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.23.0",
    "npm-run-all2": "^6.2.0",
    "prettier": "^3.2.5",
    "sass": "^1.79.4",
    "vite": "^5.3.1",
    "vite-plugin-static-copy": "^2.0.0",
    "vue-tsc": "^2.0.21"
