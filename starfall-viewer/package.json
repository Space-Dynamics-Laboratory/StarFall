    "cesium": "^1.124.0",
    "d3": "^7.9.0",
    "starfall-common": "1.0.0",
    "html2canvas": "^1.4.1",
    "jszip": "^3.10.1",
    "pinia": "^2.1.7",
    "rainbowvis.js": "^1.1.1",
    "socket.io-client": "^4.8.0",
    "splitpanes": "^3.1.5",
    "tinycolor2": "^1.6.0",
